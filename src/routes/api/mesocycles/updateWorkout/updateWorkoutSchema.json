{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"properties": {
		"mesoIndex": {
			"type": "number"
		},
		"splitExercises": {
			"items": {
				"properties": {
					"muscleTarget": {
						"enum": [
							"Back",
							"Biceps",
							"Calves",
							"Chest",
							"Forearms",
							"Front delts",
							"Glutes",
							"Hamstrings",
							"Quads",
							"Rear delts",
							"Side delts",
							"Traps",
							"Triceps"
						],
						"type": "string"
					},
					"name": {
						"type": "string"
					},
					"repRangeEnd": {
						"type": "number"
					},
					"repRangeStart": {
						"type": "number"
					},
					"setType": {
						"enum": ["down", "drop", "giant", "myorep", "myorep match", "straight", "top"],
						"type": "string"
					},
					"sets": {
						"type": "number"
					}
				},
				"type": "object"
			},
			"type": "array",
			"minItems": 1
		},
		"workoutIndex": {
			"type": "number"
		}
	},
	"type": "object"
}
